@model IEnumerable<string>

@{
    Layout = null;
}

@Html.ActionLink("Home", "List", "Product")

@foreach (var link in Model) {

    @Html.RouteLink(link, new 
        {
            controller = "Product".Trim(),
                action = "List".Trim(),
                category = link.Trim(),
                page = 1
        }, 
        new
        {
            @class = link == ViewBag.SelectedCategory ? "selected" : null
        }
    )
}